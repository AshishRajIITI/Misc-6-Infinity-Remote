{
  "content_scripts": [
    {
      "matches": ["<all_urls>", "chrome://*/*"],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ],
  "name": "Infinity Remote 2",
  "manifest_version": 2,
  "version": "2.0",
  "description": "Connect to Youtube",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
